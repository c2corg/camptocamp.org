  gpx:
    # simplify using douglas peuker algorithm (aka crosstrack for gpsbabel)
    # its value is expressed in meters (epsilon):
    filter_simplify_switch:   true
    filter_simplify_value:    3
    # hdop is horizontal dilution of precision in GPX files
    # this filter is useful to remove unprecise points:
    filter_hdop_switch:       true
    filter_hdop_value:        4    

  upload_dir: uploads

  mime_file: '../misc/magic.mime'
  
  images:
    directory_name:      images
    temp_directory_name: images_temp
    tool:                imagemagick # gd or imagemagick
    svg_rasterizer:      inkscape # batik, convert, inkscape or rsvg
    upload_method:       plupload # js, plupload, trad
    validation:
      weight:          2097152                                 #in o | = 2mo
      mime_types:      ['image/jpg', 'image/jpeg', 'image/pjpeg', 'image/gif', 'image/png', 'image/svg+xml']
      file_extensions: ['jpg', 'jpeg', 'gif', 'png', 'svg']
      max_size:            { height: 2048, width: 8192 }           #in px
      min_size:            { height: 16, width: 16 }
    types:             
      small:           
        suffix:        SI
        dimensions:    { height: 120, width: 120 }            #in px
        square:        true
        inflate:       false
        strip:         true
        progressive:   false
      medium:
        suffix:        MI
        dimensions:    { height: 250, width: 250 }            #in px
        square:        false
        inflate:       false
        strip:         true
        progressive:   true
      big:                                                     
        suffix:        BI                                      
        dimensions:    { height: 800, width: 800 }            #in px
        square:        false
        inflate:       false
        strip:         false
        progressive:   true
    home_categories: [ 1, 3, 4, 6, 7, 10, 11, 12, 16 ]
